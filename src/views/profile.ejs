<% include partials/header %>

<div class="container p-4">
    <div class="row col-md-4 offset-md-4">
        <div class="card">
            <img src="/<%= image.path %>" class="card-img-top">
            <div class="card-body">
                <h5 class="card-title">
                    <%= image.title %>
                </h5>
                <p class="card-text">
                    <%= image.description %>
                </p>
                <p class="card-text">
                    <%= format(image.create_at) %>
                </p>
                <a href="/profile/<%= image._id %>/delete" class="btn btn-danger">Eliminar Imagen</a>
            </div>
        </div>
    </div>
</div>


<% include partials/footer %>